#define BASE 0
#define MOUSE 1
#define SCROLL 2
#define SNIPE 3
#define KDL_Q_0 4
#define KDL_U_7 5
#define KDL_A_10 6
#define KDL_R_11 7
#define KDL_S_12 8
#define KDL_T_13 9
#define KDL_G_14 10
#define KDL_M_15 11
#define KDL_NN_16 12
#define KDL_E_17 13
#define KDL_I_18 14
#define KDL_O_19 15
#define KDL_SLH_30 16
#define LOWER  17
#define RAISE  18
#define HOMEROW 19
#define NAVOVERR 20
#define DIRECTACC 21
#define FUNC 22
#define ADJ  23
#define NUMBERS 24

&lt {
    tapping-term-ms = <240>;
    flavor = "balanced";
    quick-tap-ms = <150>;
};

&mt {
    tapping-term-ms = <200>;
    flavor = "tap-preferred";
    quick-tap-ms = <150>;
};

&caps_word { continue-list = <UNDERSCORE MINUS>; };

/ {

    #include "macros/macros_general.dtsi"
    #include "behaviors/behaviors_general.dtsi"
    #include "combos/combos_general.dtsi"

    keymap {
        compatible = "zmk,keymap";
 base_layer {
            label = "BASE";
            bindings = <
                               &kh KDL_Q_0 Q         &kp W             &kp F             &kp P            &kp B                           &kp J             &kp L              &kh KDL_U_7 U           &kp Y                &kp SQT
                               &kh KDL_A_10 A        &kh KDL_R_11 R    &kh KDL_S_12 S    &kh KDL_T_13 T   &kh KDL_G_14 G                  &kh KDL_M_15 M    &kh KDL_NN_16 N    &kh KDL_E_17 E          &kh KDL_I_18 I       &kh KDL_O_19 O
                               &kp Z                 &kp X             &kp C             &kp D            &kp V                           &kp K             &kp H              &kp COMMA               &kp DOT              &kp SLASH             
                               &mo SCROLL  &trans  &trans &trans  &lm_homerow_short LSHFT BACKSPACE  &lm_homerow_short LGUI TAB                &base_th_mo_kp_hp RAISE ENTER  &spacelm NAVOVERR SPACE  &mo SCROLL
            >;
        };
        mouse_layer {
            label = "MOUSE";
            bindings = <
&kp N1          &kp N2          &kp N3        &kp N4     &kp N5                  &kp N6         &kp N7     &kp N8  &kp N9     &kp N0
&kp LEFT_ARROW  &kp DOWN_ARROW  &kp UP_ARROW  &kp RIGHT  &none                   &kp PAGE_UP    &mkp LCLK  &none   &mkp RCLK  &mkp MCLK
&none           &none           &none         &none      &none                   &kp PAGE_DOWN  &none      &none   &none      &none
&none           &trans          &trans        &trans     &trans  &trans  &trans  &mo 3                                        &to BASE
            >;
        };

         scroll_layer {
            label = "SCROLL";
            bindings = <
&trans  &trans  &trans  &trans  &trans                  &trans  &trans  &trans  &trans  &trans
&trans  &trans  &trans  &trans  &trans                  &trans  &trans  &trans  &trans  &trans
&trans  &trans  &trans  &trans  &trans                  &trans  &trans  &trans  &trans  &trans
&trans  &trans  &trans  &trans  &trans  &trans  &trans  &trans                          &trans
            >;
        };

        snipe_layer {
            label = "SNIPE";
            bindings = <
&trans  &trans  &trans  &trans  &bootloader                  &trans  &trans  &trans  &trans  &trans
&trans  &trans  &trans  &trans  &trans                       &trans  &trans  &trans  &trans  &trans
&trans  &trans  &trans  &trans  &trans                       &trans  &trans  &trans  &trans  &trans
&trans  &trans  &trans  &trans  &trans       &trans  &trans  &trans                          &trans
            >;
        };

         layer_KDL_Q_0 {
            label = "KDL_Q_0";
            bindings = <
        &trans  &trans  &trans  &trans  &trans    &trans  &trans  &trans    &trans  &trans
        &trans  &trans  &trans  &trans  &trans    &trans  &trans  &trans    &trans  &trans
        &trans  &trans  &trans  &trans  &trans    &trans  &trans  &trans    &trans  &trans  
        &trans  &trans  &trans &trans  &trans  &kp LA(N1)    &trans  &trans  &trans
            >;
        };

layer_kdl_u_7 {
            label = "KDL_U_7";
            bindings = <
        &trans  &trans  &trans  &trans  &trans           &trans  &trans  &trans  &trans  &trans
        &trans  &trans  &trans  &trans  &trans            &trans  &trans  &trans  &trans  &trans
        &trans  &trans  &trans  &trans  &trans            &trans  &trans  &trans  &trans  &trans 
        &trans  &trans  &trans  &trans  &mc_bkctilde U  &trans    &trans  &trans  &trans
            >;
        };

layer_kdl_a_10 {
            label = "KDL_A_10";
            bindings = <
        &trans  &trans  &trans  &trans   &trans       &trans  &trans  &trans  &trans  &trans
        &trans  &trans  &trans  &trans   &trans       &trans  &trans  &trans  &trans  &trans
        &trans  &trans  &trans  &trans   &trans       &trans  &trans  &trans  &trans  &trans  
        &trans  &trans  &trans &trans  &mc_bkctilde A  &lm_mac_homerow LSHFT     &trans  &trans  &trans
            >;
        };


layer_kdl_r_11 {
            label = "KDL_R_11";
            bindings = <
        &trans  &trans  &trans  &trans  &trans       &trans  &trans  &trans  &trans  &trans
        &trans  &trans  &trans  &trans  &trans       &trans  &trans  &trans  &trans  &trans
        &trans  &trans  &trans  &trans  &trans       &trans  &trans  &trans  &trans  &trans  
        &trans  &trans  &trans  &trans  &trans  &lm_mac_homerow LCTRL    &trans  &trans  &trans
            >;
