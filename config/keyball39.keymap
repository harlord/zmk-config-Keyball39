layer_kdl_t_13 {
            label = "KDL_T_13";
            bindings = <
        &trans  &trans  &trans  &trans  &trans      &trans  &trans  &trans  &trans  &trans
        &trans  &trans  &trans  &trans  &trans      &trans  &trans  &trans  &trans  &trans
        &trans  &trans  &trans  &trans  &trans      &trans  &trans  &trans  &trans  &trans  
        &trans  &trans  &trans &trans  &trans  &lm_mac_homerow LGUI    &trans  &trans  &trans
            >;
        };
 
layer_kdl_g_14 {
     label = "KDL_G_14";
            bindings = <
        &trans  &trans  &trans  &trans  &trans       &trans  &trans  &trans  &trans  &trans
        &trans  &trans  &trans  &trans  &trans       &trans  &trans  &trans  &trans  &trans
        &trans  &trans  &trans  &trans  &trans       &trans  &trans  &trans  &trans  &trans  
        &trans  &trans  &trans &trans  &trans  &trans       &trans  &trans  &trans
            >;
